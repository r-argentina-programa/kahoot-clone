  },
  "homepage": "https://github.com/r-argentina-programa/kahoot-clone#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "concurrently": "^5.3.0",
    "http-server": "^0.12.3",
    "mock-socket": "^9.0.3",
    "nodemon": "^2.0.6",
    "serve": "^11.3.2"
  }
}
